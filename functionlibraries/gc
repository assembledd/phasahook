local utility = {}

function utility.findsimularfunction(name)
    for i,v in getgc() do
        if typeof(v) == "function" then
            if debug.info(v, "n"):find(name) then
                return v
            end
        end
    end
    return nil
end

function utility.findfunction(name)
    for i,v in getgc() do
        if typeof(v) == "function" and debug.info(v, "n") == name then
            return v
        end
    end
    return nil
end

return utility
